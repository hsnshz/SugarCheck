{
  "expo": {
    "name": "SugarCheck",
    "slug": "SugarCheck",
    "description": "An AI based cross-platform mobile app to help you predict and manage your diabetes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/AppIcon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/AppIcon.png",
        "backgroundColor": "background"
      }
    },
    "web": {
      "favicon": "./assets/icons/AppIcon.png"
    },
    "notifications": {
      "icon": "./assets/icons/AppIcon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    }
  }
}
